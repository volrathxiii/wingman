{
  "name": "wingman",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm install && tsc",
    "start-local:socket": "node ./build/socket.server.js --enable-source-maps",
    "start-local:web": "node ./build/web.server.js --enable-source-maps",
    "start-local": "npm run build && (npm run start-local:socket & npm run start-local:web )",
    "start-dev:socket": "pm2 start build/socket.server.js --watch",
    "start-dev:web": "pm2 start build/web.server.js --watch",
    "start-dev": "npm run build && (npm run start-dev:socket & npm run start-dev:web )",
    "start": "npm run build && pm2 start ecosystem.config.js --watch --wait-ready",
    "stop": "pm2 stop ecosystem.config.js",
    "monitor": "pm2 monit",
    "logs": "pm2 logs",
    "delete": "pm2 delete all",
    "list": "pm2 list",
    "deploy": "npm run build && pm2 start ecosystem.config.js --env production --wait-ready",
    "restart": "npm run build && pm2 restart ecosystem.config.js --env production --wait-ready"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/glob": "^7.1.3",
    "@types/natural": "^0.6.3",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/nunjucks": "^3.1.3",
    "@types/sentiment": "^5.0.0",
    "@types/ws": "^7.2.6",
    "body-parser": "^1.19.0",
    "compromise": "^13.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "moment": "^2.27.0",
    "natural": "^2.1.5",
    "node-fetch": "^2.6.0",
    "nunjucks": "^3.2.2",
    "pm2": "^4.4.0",
    "sentiment": "^5.0.2",
    "synchronized-promise": "^0.3.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "ws": "^7.3.1",
    "yargs": "^15.4.1"
  }
}
